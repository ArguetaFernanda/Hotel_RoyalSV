<Window x:Name="GenrarVista" x:Class="Hotel_RoyalSV.Ventanas.ManejarVistas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hotel_RoyalSV.Ventanas"
        mc:Ignorable="d"
        Title="Tablas" Height="450" Width="800" Icon="/Hotel_RoyalSV;component/Resources/Hotel Royal SV Logo.ico" Background="#FF202020" Closing="GenrarVista_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label x:Name="LBL_Title" x:FieldModifier="public" Grid.Row="0" Content="Tabla?" Background="#FF1D826E" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="20"/>

        <Button Grid.Row="1" x:Name="BTN_Back" Content="Volver" HorizontalAlignment="Right" Width="90" Background="#FF9D0100" Foreground="White" FontWeight="Bold" Cursor="Hand" Click="BTN_Back_Click"/>

        <Frame Grid.Row="2" x:Name="FR_Content" x:FieldModifier="public"/>
    </Grid>
</Window>
